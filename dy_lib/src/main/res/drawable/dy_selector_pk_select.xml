<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <stroke android:width="1dp" android:color="#2B000000" />
            <solid android:color="@color/dy_base_color_transparent"></solid>
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape>

            <stroke android:width="1dp" android:color="#2B000000" />

            <solid android:color="@color/dy_base_color_transparent"></solid>
        </shape>
    </item>


    <item android:state_pressed="true">
        <shape>

            <stroke android:width="1dp" android:color="#2B000000" />

            <solid android:color="#2C0B02"></solid>
        </shape>
    </item>

</selector>